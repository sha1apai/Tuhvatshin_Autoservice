<Window x:Class="Tuhvatshin_Autoservice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tuhvatshin_Autoservice"
        mc:Ignorable="d"
        Title="Тухватшин" Height="450" Width="800" FontFamily=" Tw Cen MT" Icon="res/service_logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="/res/service_logo.png" HorizontalAlignment="Left" />
        <TextBlock Text="Услуги Автосервис" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" />
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"/>
        <Button Content="Назад" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click"/>
    </Grid>
</Window>
